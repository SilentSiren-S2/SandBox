<Window x:Class="SandBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SandBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="createButton" Content="SandBox" Click="createButton_Click" Background="LimeGreen" Padding="10" Margin="10" FontFamily="Times New Roman" FontSize="20"/>
        <Button x:Name="EngineButton" Content="Engine" Click="EngineButton_Click" Background="LimeGreen" Padding="10" Margin="10" FontFamily="Times New Roman" FontSize="20"/>
        <Button x:Name="Engine2Button" Content="Engine2" Click="EngineButton2_Click" Background="LimeGreen" Padding="10" Margin="10" FontFamily="Times New Roman" FontSize="20"/>
        <Button x:Name="DeadInsideButton" Content="DeadInside_Click" Background="LimeGreen" Padding="10" Margin="10" FontFamily="Times New Roman" FontSize="20" Click="DeadInside_Click"/>

    </StackPanel>
</Window>
